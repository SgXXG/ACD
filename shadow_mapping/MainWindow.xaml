<Window x:Name="window" x:Class="ACG_1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ACG_1"
        mc:Ignorable="d"
        Title="MainWindow" Width="1920" Height="1080" ResizeMode="NoResize" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Left="0" Top="0">
    <Grid x:Name="grid">
        <Canvas x:Name="canvas">
            <Image x:Name="image" Height="1080" Canvas.Left="0" Canvas.Top="0" Width="1920" RenderTransformOrigin="1,1"/>
        </Canvas>

    </Grid>
</Window>
